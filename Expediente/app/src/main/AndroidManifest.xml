<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expediente">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_disease_prevention"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_disease_prevention"
        android:supportsRtl="true"
        android:theme="@style/Theme.Expediente"
        android:usesCleartextTraffic="true">

        <activity android:name=".InicioSesion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuPrincipal"></activity>
        <activity android:name=".Vaccines" />
        <activity android:name=".VaccineInfo"/>

        <activity android:name=".Allergies" />
        <activity android:name=".AllergyInfo" />

        <activity android:name=".Appointments" />
        <activity android:name=".AppointmentInfo" />

        <activity android:name=".UserInfo" />
        <activity android:name=".Registro" />



    </application>

</manifest>